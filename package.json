    "jeffwillette"
  ],
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop --port 8001",
    "clean": "rm -r ./.cache",
    "get-schema": "apollo service:download --endpoint http://localhost:8001/___graphql ./gatsby-schema.json",
    "gatsby-gen": "apollo client:codegen --addTypename --outputFlat --tagName=graphql --target=typescript --localSchemaFile=./gatsby-schema.json  --includes='./{src,.cache}/**/*.{ts,tsx,js,jsx}' ./src/gatsby-queries.d.ts",
    "gen": "yarn get-schema && yarn gatsby-gen",
    "format": "prettier --write \"src/**/*.ts*\"",
    "test": "jest --coverage && yarn typecheck && yarn lint",
    "typecheck": "tsc --noEmit",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.36",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "prettier": "^2.0.5",
    "typescript": "^3.9.5",
    "webpack-bundle-analyzer": "^3.8.0"
  }
}
